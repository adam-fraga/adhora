package layouts

import (
met "github.com/adam-fraga/adhora/views/metadatas"
par "github.com/adam-fraga/adhora/views/partials"
t "github.com/a-h/templ"
)

var (
Head = met.Head
Header = par.Header
Footer = par.Footer
)

templ Base(Page t.Component) {
<!DOCTYPE html>
<html>
@Head()
@Header()

<body class="bg-gray-100 text-gray-900">
  @Page
</body>

@Footer()

</html>
}
